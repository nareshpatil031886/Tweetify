{
 "builds": [{
     "src": "twitter_backend/wsgi.py",
     "use": "@vercel/python",
     "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
 }],
 "hooks": {
    "build": "./build.sh"
  },
 "routes": [
     {
         "src": "/(.*)",
         "dest": "twitter_backend/wsgi.py"
     }
 ]
}
